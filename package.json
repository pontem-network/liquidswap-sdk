{
  "name": "@pontem/liquidswap-sdk",
  "version": "0.7.3",
  "description": "SDK to use LiquidSwap functions",
  "author": "Igor Demko <igor@pontem.network>",
  "repository": "https://github.com/pontem-network/liquidswap-sdk",
  "license": "GPL-3.0",
  "keywords": [
    "aptos",
    "blockchain",
    "crypto",
    "curve",
    "DEX",
    "exchange",
    "liquidswap",
    "pontem",
    "sdk",
    "tokens"
  ],
  "main": "./dist/tsc/main.js",
  "types": "./dist/tsc/main.d.ts",
  "scripts": {
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "clean": "rm -rf dist build package",
    "ts-node": "ts-node",
    "docs": "typedoc --entryPoints src/main.ts",
    "format": "prettier --write 'src/**/*.{js,ts,json}' '*.{js,ts,json}'",
    "build": "tsc -p tsconfig.json",
    "build-all": "yarn clean && yarn build"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.26",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "esbuild": "^0.14.38",
    "eslint": "^8.14.0",
    "jest": "27.0.0",
    "prettier": "^2.8.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "aptos": "1.21.0",
    "axios": "^0.27.2",
    "decimal.js": "^10.4.1"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
